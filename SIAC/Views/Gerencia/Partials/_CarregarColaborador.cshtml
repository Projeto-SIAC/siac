@model GerenciaEditarColaboradorViewModel

<i class="icon close"></i>
<div class="header">
    Editar Colaborador "@Model.Colaborador.Usuario.PessoaFisica.Nome"
</div>
<div class="content">
    <div class="ui form">
        <div class="field required">
            <label data-ajuda="">Informe o nome completo</label>
            <input type="text" name="txtNome" placeholder="Nome" maxlength="100" required value="@Model.Colaborador.Usuario.PessoaFisica.Nome" />
        </div>
    </div>
</div>
<div class="actions">
    <input type="reset" value="Cancelar" class="ui cancel button" />
    <button type="submit" class="ui button">Salvar</button>
</div>
